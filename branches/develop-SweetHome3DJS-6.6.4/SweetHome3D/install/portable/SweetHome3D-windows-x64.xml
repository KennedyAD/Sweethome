<!-- SweetHome3D-windows-x64.xml

     Sweet Home 3D, Copyright (c) 2012-2021 Emmanuel PUYBARET / eTeks <info@eteks.com>

     SweetHome3D-windows-x64.exe creator for Launch4j 
     This script requires launch4j available at http://launch4j.sourceforge.net/     
     and a SweetHome3D.ico file containing Sweet Home 3D icon.
     It creates a SweetHome3D-windows-x64.exe in the current directory running with
     JRE or OpenJDK stored in runtime/windows/x64/runtime subdirectory 
     + SweetHome3D.jar, Windows Java 3D DLLs and JARs for Java 3D and other required JARs 
       stored in a SweetHome3D-macosx.app/Contents/app subdirectory -->
<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar></jar>
  <outfile>SweetHome3D-windows-x64.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>true</customProcName>
  <stayAlive>false</stayAlive>
  <icon>../windows/SweetHome3D.ico</icon>
  <classPath>
    <mainClass>com.eteks.sweethome3d.SweetHome3D</mainClass>
    <cp>SweetHome3D-macosx.app/Contents/app/SweetHome3D.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/Furniture.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/Textures.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/Examples.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/Help.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/batik-svgpathparser-1.7.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/jeksparser-calculator.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/iText-2.1.7.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/freehep-vectorgraphics-svg-2.1.1b.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/sunflow-0.07.3i.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/ext/jmf.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/ext/java3d-1.6/j3dcore.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/ext/java3d-1.6/j3dutils.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/ext/java3d-1.6/vecmath.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/ext/java3d-1.6/gluegen-rt.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/ext/java3d-1.6/jogl-java3d.jar</cp>
    <cp>SweetHome3D-macosx.app/Contents/app/ext/jnlp.jar</cp>
  </classPath>
  <jre>
    <path>runtime/windows/x64/runtime</path>
    <bundledJre64Bit>true</bundledJre64Bit>
    <minVersion>1.5.0</minVersion>
    <maxVersion></maxVersion>
    <dontUsePrivateJres>false</dontUsePrivateJres>
    <initialHeapSize>0</initialHeapSize>
    <maxHeapSize>2048</maxHeapSize>
    <maxHeapPercent>50</maxHeapPercent>
    <opt>-Djava.library.path=SweetHome3D-macosx.app/Contents/app/ext/java3d-1.6/windows/amd64 -Djogamp.gluegen.UseTempJarCache=false -Djava.locale.providers="COMPAT,SPI" -Dcom.eteks.sweethome3d.preferencesFolder=data -Dcom.eteks.sweethome3d.applicationFolders=data -Dcom.eteks.sweethome3d.deploymentInformation=portable -Dcom.eteks.sweethome3d.applicationId=SweetHome3D#Portable</opt>
  </jre>
  <versionInfo>
    <fileVersion>6.6.4.0</fileVersion>
    <txtFileVersion>6.6.4</txtFileVersion>
    <fileDescription>Sweet Home 3D</fileDescription>
    <copyright>Copyright (c) 2007-2021 eTeks</copyright>
    <productVersion>6.6.4.0</productVersion>
    <txtProductVersion>6.6.4</txtProductVersion>
    <productName>Sweet Home 3D</productName>
    <companyName>eTeks</companyName>
    <internalName>SweetHome3D</internalName>
    <originalFilename>SweetHome3D.exe</originalFilename>
  </versionInfo>
</launch4jConfig>